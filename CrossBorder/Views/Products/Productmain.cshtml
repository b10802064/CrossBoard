@model IEnumerable<Product>

@{
    ViewData["Title"] = "Productmain";
}

<div class="productsidebar">
    <button>長褲</button>
    <button>短褲</button>
    <button>長裙</button>
    <button>T-Shirt</button>
    <button>洋裝</button>
    <button>長袖</button>
    <button>短袖</button>
    <button>外套</button>
    <!-- 其他类别按钮 -->
</div>
<div class="productcontent">
    <!---->
    <h2>商品總覽</h2>
    <div class="line"></div>
    <div class="row">
        @foreach (var man in Model)
        {
            @await Html.PartialAsync("_CardPartial", man)
        }
    </div>

</div>



@section topCSS{
    <link href="~/css/productleft.css" rel="stylesheet" />
    <link href="~/css/Card.css" rel="stylesheet" />
}